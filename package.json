{
  "name": "weathy",
  "version": "1.0.0",
  "description": "Mobile weather forecast app created using react and nodejs and free openweathermap api",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev-server\" \"npm:dev-react\"",
    "dev-server": "cross-env NODE_ENV=development nodemon --watch server server/index.js",
    "dev-react": "cd react-frontend && cross-env NODE_ENV=development npm start"
  },
  "keywords": [
    "React",
    "Weather",
    "forecast",
    "app",
    "Nodejs",
    "OpenWatherMap"
  ],
  "author": "Akmal Maulana Azzam",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "nodemon": "2.0.3"
  }
}
